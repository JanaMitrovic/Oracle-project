ALTER TRIGGER "JANA"."UPD_AKTUELNA_CENA_ZABRANA" ENABLE;
--------------------------------------------------------
--  DDL for Trigger UPD_DATUM_IZDAVANJA
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE TRIGGER "JANA"."UPD_DATUM_IZDAVANJA" 
AFTER UPDATE OF DatumIzdavanja ON Nalog
FOR EACH ROW
DECLARE
PRAGMA AUTONOMOUS_TRANSACTION;
BEGIN
EXECUTE IMMEDIATE 'ALTER TRIGGER UPD_DATUM_ZABRANA DISABLE';
UPDATE StavkaNaloga 
SET DatumIzdavanja = :NEW.DatumIzdavanja
WHERE Broj = :NEW.Broj AND Godina = :NEW.Godina;
BEGIN
EXECUTE IMMEDIATE 'ALTER TRIGGER UPD_DATUM_ZABRANA ENABLE';
END;
END;
/
ALTER TRIGGER "JANA"."UPD_DATUM_IZDAVANJA" ENABLE;
