--------------------------------------------------------
--  DDL for Trigger INSTEAD_DEL_ZAPISNIK
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE TRIGGER "JANA"."INSTEAD_DEL_ZAPISNIK" 
INSTEAD OF DELETE ON Zapisnik
FOR EACH ROW
BEGIN
DELETE FROM Zapisnik_osnovno WHERE Broj = :OLD.Broj AND Godina = :OLD.Godina;
DELETE FROM Zapisnik_detalji WHERE Broj = :OLD.Broj AND Godina = :OLD.Godina;
END;
/
ALTER TRIGGER "JANA"."INSTEAD_DEL_ZAPISNIK" ENABLE;
--------------------------------------------------------
--  DDL for Trigger INSTEAD_INS_ZAPISNIK
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE TRIGGER "JANA"."INSTEAD_INS_ZAPISNIK" 
INSTEAD OF INSERT ON Zapisnik
FOR EACH ROW
BEGIN
INSERT INTO Zapisnik_osnovno(Broj, Godina, IdFilijala, PIB)
VALUES (:NEW.Broj, :NEW.Godina, :NEW.IdFilijala, :NEW.PIB);
INSERT INTO Zapisnik_detalji(Broj, Godina, SifraObrazac, DatumDonosenja, DatumPrijema)
VALUES (:NEW.Broj, :NEW.Godina, :NEW.SifraObrazac, :NEW.DatumDonosenja,:NEW.DatumPrijema);
END;
/
ALTER TRIGGER "JANA"."INSTEAD_INS_ZAPISNIK" ENABLE;
--------------------------------------------------------
--  DDL for Trigger INSTEAD_UPD_ZAPISNIK
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE TRIGGER "JANA"."INSTEAD_UPD_ZAPISNIK" 
INSTEAD OF UPDATE ON Zapisnik
FOR EACH ROW
BEGIN
UPDATE Zapisnik_osnovno 
SET IdFilijala = :NEW.IdFilijala, PIB = :NEW.PIB
WHERE Broj = :NEW.Broj AND Godina = :NEW.Godina;
UPDATE Zapisnik_detalji 
SET SifraObrazac = :NEW.SifraObrazac, DatumDonosenja = :NEW.DatumDonosenja, DatumPrijema = :NEW.DatumPrijema
WHERE Broj = :NEW.Broj AND Godina = :NEW.Godina;
END;
/
ALTER TRIGGER "JANA"."INSTEAD_UPD_ZAPISNIK" ENABLE;
--------------------------------------------------------
--  DDL for Trigger INS_NALOG
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE TRIGGER "JANA"."INS_NALOG" 
BEFORE INSERT ON Nalog
FOR EACH ROW
DECLARE v_mesto NUMBER(10);
BEGIN
SELECT IdMesto INTO v_mesto FROM Filijala WHERE IdFilijala = :NEW.IdFilijala;
:NEW.IdMesto := v_mesto;
END;
/
ALTER TRIGGER "JANA"."INS_NALOG" ENABLE;
--------------------------------------------------------
--  DDL for Trigger INS_STAVKA_NALOGA
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE TRIGGER "JANA"."INS_STAVKA_NALOGA" 
BEFORE INSERT ON StavkaNaloga
FOR EACH ROW
DECLARE v_datumIzdavanja DATE;
BEGIN
SELECT DatumIzdavanja INTO v_datumIzdavanja FROM Nalog WHERE Broj =: NEW.Broj AND Godina = :NEW.Godina;
:NEW.DatumIzdavanja := v_datumIzdavanja;
END;
/
ALTER TRIGGER "JANA"."INS_STAVKA_NALOGA" ENABLE;
--------------------------------------------------------
--  DDL for Trigger ROBA_CENA1
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE TRIGGER "JANA"."ROBA_CENA1" 
BEFORE INSERT OR UPDATE OR DELETE ON Cena
FOR EACH ROW
BEGIN
IF (INSERTING OR UPDATING)
THEN
BEGIN 
RobaPaket.IdRoba := :NEW.IdRoba; 
END;
ELSE
BEGIN 
RobaPaket.IdRoba := :OLD.IdRoba; 
END;
END IF;
END;
/
ALTER TRIGGER "JANA"."ROBA_CENA1" ENABLE;
--------------------------------------------------------
--  DDL for Trigger ROBA_CENA2
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE TRIGGER "JANA"."ROBA_CENA2" 
AFTER INSERT OR UPDATE OR DELETE ON Cena
DECLARE
Roba NUMBER(10) := RobaPaket.IdRoba;
BEGIN
ISKLJUCI_TRIGER_CENA();
AKTUELNA_CENA_ROBE(Roba);
UKLJUCI_TRIGER_CENA();
END;
/
ALTER TRIGGER "JANA"."ROBA_CENA2" ENABLE;
--------------------------------------------------------
--  DDL for Trigger UKUPAN_IZNOS_RESENJE1
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE TRIGGER "JANA"."UKUPAN_IZNOS_RESENJE1" 
BEFORE INSERT OR UPDATE OR DELETE ON StavkaResenja 
FOR EACH ROW
BEGIN
IF (INSERTING OR UPDATING)
THEN
BEGIN 
RStavka.Broj := :NEW.Broj;
RStavka.Godina := :NEW.Godina; 
END;
ELSE
BEGIN 
RStavka.Broj := :OLD.Broj;
RStavka.Godina := :OLD.Godina; 
END;
END IF;
END;
/
ALTER TRIGGER "JANA"."UKUPAN_IZNOS_RESENJE1" ENABLE;
--------------------------------------------------------
--  DDL for Trigger UKUPAN_IZNOS_RESENJE2
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE TRIGGER "JANA"."UKUPAN_IZNOS_RESENJE2" 
AFTER INSERT OR UPDATE OR DELETE ON StavkaResenja
DECLARE
Broj NUMBER(10) := RStavka.Broj;
Godina NUMBER(10) := RStavka.Godina;
BEGIN
ISKLJUCI_TRIGER();
UKUPAN_IZNOS_RESENJA(Broj, Godina);
UKLJUCI_TRIGER();
END;
/
ALTER TRIGGER "JANA"."UKUPAN_IZNOS_RESENJE2" ENABLE;
